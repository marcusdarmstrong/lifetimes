{
  "name": "@lifetimes/eslint-plugin",
  "version": "3.0.0",
  "author": "Marcus Armstrong <marcusdarmstrong@gmail.com>",
  "homepage": "https://github.com/marcusdarmstrong/lifetimes",
  "repository": "github:marcusdarmstrong/lifetimes",
  "license": "MIT",
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js"
  },
  "main": "dist/index.js",
  "keywords": ["eslint", "eslintplugin"],
  "files": [
    "package.json",
    "dist/index.js",
    "dist/index.js.map",
    "README.md"
  ],
  "dependencies": {
    "@typescript-eslint/utils": ">=8.0.0"
  },
  "peerDependencies": {
    "eslint": ">=5.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-swc": "0.3.0",
    "@rollup/plugin-eslint": "9.0.5",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@swc/core": "1.5.24",
    "@tsconfig/strictest": "2.0.5",
    "@tsconfig/node20": "20.1.6",
    "@types/eslint": "8.56.10",
    "@types/node": "20.14.8",
    "@typescript-eslint/types": "8.40.0",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "@typescript-eslint/rule-tester": "8.40.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "rollup": "4.18.0",
    "typescript": "5.9.2"
  },
  "scripts": {
    "build": "tsc && rollup -c && node --test dist/index.test.mjs"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ]
  }
}
